<div class="container-fluid">

	<!--Carousel -->

	<div class="row">
		<div class="col-md-12" style="padding-right: 0; padding-left: 0;">
		<div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
			<!-- Indicators -->
			<ol class="carousel-indicators">
				<li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
				<li data-target="#carousel-example-generic" data-slide-to="1"></li>
				<li data-target="#carousel-example-generic" data-slide-to="2"></li>
			</ol>

			<!-- Wrapper for slides -->
			<div class="carousel-inner" role="listbox" max-width="600" max-height="400">
				<div class="item active" >
					<img src="https://s3-us-west-1.amazonaws.com/yumster/rsz_thai_food.jpg" id="carousel-img">
					<div class="carousel-caption">
						<h4> Join a community of food lovers and cooks. Experience unique meals together.</h4>
					</div>
				</div>
		
				<div class="item">
					<img src="https://s3-us-west-1.amazonaws.com/yumster/rsz_candle-light.jpg" id="carousel-img">
					<div class="carousel-caption">
						<h4> Join a community of food lovers and cooks. Experience unique meals together.</h4>
					</div>
				</div>
		
				<div class="item">
					<img src="https://s3-us-west-1.amazonaws.com/yumster/rsz_mexican.jpg" id="carousel-img">
					<div class="carousel-caption">
						<h4> Join a community of food lovers and cooks. Experience unique meals together.</h4>
					</div>
				</div>
		
			</div>

			<!-- Controls -->
			<a class="left carousel-control" href="#carousel-example-generic" role="button" data-slide="prev">
				<span class="glyphicon glyphicon-chevron-left"></span>
				<span class="sr-only">Previous</span>
			</a>
			<a class="right carousel-control" href="#carousel-example-generic" role="button" data-slide="next">
				<span class="glyphicon glyphicon-chevron-right"></span>
				<span class="sr-only">Next</span>
			</a>
		</div>
		</div>
	</div>
	
	<!--Search Bar -->
	<div class="row search-div">
		<div class="col-md-10 col-md-offset-1">
			<form>
		    <input type="hidden" id="lat-search" name="lat">
				<input type="hidden" id="lng-search" name="lng">
				<select class="search-bar" name="tag">
					<option value="" disabled selected>What are you craving?</option>
					<% for (var item in cuisines) { %>
						<option value="<%= cuisines[item] %>"><%= cuisines[item] %></option>
					<% } %>
				</select>
		
				<input id="location-search" type="text" name="location" class="search-bar" placeholder="Location" style="width: 230px;">
		    <input placeholder="Start Date" name="start_date" class="search-bar" class="textbox-n" type="text" onfocus="(this.type='date')">
				<input placeholder="End Date" name="end_date" class="search-bar" class="textbox-n" type="text" onfocus="(this.type='date')">
				<!-- <input type="date" name="start_date" class="search-bar">

				<input type="date" name="end_date" class="search-bar"> -->
		
				<select class="search-bar" name="guest_num">
					<option value="1" selected>1 Guest</option>
					<option value="2">2 Guests</option>
					<option value="3">3 Guests</option>
					<option value="4">4 Guests</option>
					<option value="5">5+ Guests</option>
				</select>
					
				<button type="submit" id="button-search" class="search-button btn btn-default" >Search </button>
		
			</form>
		</div>
	</div>
	
	<!--Explore your Neighborhood Header -->
	
	<div class="row" ><!-- style="margin-top: 15px;"> -->
		<div class="col-md-8 col-md-offset-4 head">
			<h2> Explore your Neighborhood </h2>
			<h5> <i>...what tantalizing tastes await around the corner </i> </h5>
		</div>
	</div>
	
	<!-- Meal Info Samples -->
	
	<div class="row" style="margin-left: 2vw; margin-bottom: 4vh;">
		
		<div class="col-md-3 col-md-offset-1 meal-info" style="background-image: url('https://s3-us-west-1.amazonaws.com/yumster/seafood.jpg');">
			<div class="host-profile">
				<img src="https://s3-us-west-1.amazonaws.com/yumster/host1.jpg" class="host-pic">
				<span style="float: left; margin-left: 5px;">
					Natasha's
				</span>
			</div>
			<div class="meal-blurb">
				<p class="tag">LATIN AMERICAN</p>
				<p> Under the Sea </p>
				<p style="font-size: 10px;">
				  <span style="float: left;"> @Mission </span>
					<span style="float: right;">$25/p </span>
				</p>
			</div>
		</div>
		
				<div class="col-md-3 col-md-offset-1 meal-info" style="background-image: url('https://s3-us-west-1.amazonaws.com/yumster/quiche.jpg');">
					
					<div class="host-profile">
