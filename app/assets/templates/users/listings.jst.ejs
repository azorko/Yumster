<h3> Listings </h3>

<% listings.forEach( function (meal) { %>
<div class="row">
	<h3 class="meal-header"> <%= meal.get("title") %> </h3>
	<div class="meal-div">
		<div class="col-md-3">
	    <div class="user-meal meals" style="background-image: url('<%= meal.get("photo_url") %>');"></div>
		</div>
		<div class="col-md-9">
			<div class="row">
				<span class="glyphicon glyphicon-calendar"> </span>
				<%= meal.get("date") %>
			</div>
				<!-- <span class="glyphicon glyphicon-calendar"> </span> -->
			<div class="row">
				<span class="glyphicon glyphicon-map-marker"> </span>
				<%= user.get("address") %>
			</div>
		</div>	
	</div>
</div>
<% }) %>